<%= form_for(@profile) do |f| %>
  <% if @profile.errors.any? %>
    <% @profile.errors.full_messages.each do |message| %>
      <%= message %>
    <% end %>
  <% end %>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br>

  <%= f.submit %>
<% end %>
<br>

